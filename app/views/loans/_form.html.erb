<%= form_for(@loan, :html => {:class => "form-inline"}) do |f| %>
  <% if @loan.errors.any? %>
    <div id="error_explaination">
      <h2><%= /#{pluralize(@loan.errors.count, "error")} stopped this loan from being saved:/ %></h2>
      <ul>
        <% @loan.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  
  <div>
    <%= email_field :borrower, :email, :placeholder => "email address"  %>
  </div>
  <div><input id="foo" type="text" autocomplete="off" ></div>
  

  <script>
  $('#foo').typeahead({
      source: function (query, process) {
          return $.get('/items.json', { query: query }, function (data) {
          //   alert(data);
            return process(data);
          });
      }
  });
   </script>
  <div>
    
    <%= f.autocomplete_field :item_name, autocomplete_item_name_loans_path, :placeholder => "item" %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>