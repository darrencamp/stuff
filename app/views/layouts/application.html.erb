<!DOCTYPE html>
<html>
<head>
  <title>Stuff</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= render 'shared/navigation' %>
    <div class="container">
		<%- flash.each do |name, msg| -%>
			<%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
		<%- end -%>
		<%= yield %>
	</div>
</body>
</html>
